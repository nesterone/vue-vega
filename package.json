{
  "name": "vue-vega",
  "version": "1.0.0-alpha.11",
  "description": "Vega Lite and Vega bridge to Vue.js ecosystem",
  "main": "dist/vue-vega.min.js",
  "unpkg": "dist/vue-vega.min.js",
  "bugs": {
    "url": "https://github.com/nesterone/vue-vega/issues"
  },
  "homepage": "https://nesterone.github.io/vue-vega",
  "author": "nesterone <igornester@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nesterone/vue-vega.git"
  },
  "license": "MIT",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build:bundle": "rm -rf dist && BUILD=web webpack --config build/webpack.bundle.conf.js",
    "build:lib": "rm -rf lib && BUILD=lib babel src --out-dir lib",
    "build:docs": "rm -rf docs && mkdir docs && node build/docs.js",
    "build:docs:data": "rm -rf docs/data && mkdir docs/data && cpy 'data/*.json' 'docs/data/' && cpy 'data/*.csv' 'docs/data/'",
    "build": "npm run build:lib && npm run build:bundle && npm run build:docs && npm run build:docs:data",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs"
  },
  "dependencies": {
    "lodash-es": "^4.17.4",
    "vega": "^3.0.2",
    "vega-lite": "^2.0.0-rc3",
    "vue": "^2.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cpy-cli": "^1.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
